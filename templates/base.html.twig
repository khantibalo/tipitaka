<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        {% block meta %}{% endblock %}
        <title>{% block title %}{% endblock %}</title>
        {% block stylesheets %}
        <link href="{{ asset('styles.css') }}" rel="stylesheet"/>
        <link href="{{ asset('favicon.png') }}" rel="shortcut icon"/>
        {% endblock %}
    </head>
    <body>
        {% block navigation %}
        {{ include('_navigation.html.twig') }}
        {% endblock %}
        {% block content %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
